{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
  },
  "source": {
      "include": ["lib", "README.md"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "templates": {
      "referenceTitle": "AWS APN Connector",
      "disableSort": false,
      "collapse": true
  },
  "opts": {
      "destination": "./docs/",
      "encoding": "utf8"
      // "template": "node_modules/better-docs"
  }
}